{
  "name": "rogue-boi",
  "version": "0.0.1",
  "scripts": {
    "build": "cargo xtask clean && rollup -c --environment BUILD:production",
    "build:dev": "cargo xtask clean && rollup -c --environment BUILD:dev",
    "dev": "rollup -c -w",
    "start": "sirv public",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore ui",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "bundle": "cargo xtask copy-icons && npm run build && cargo xtask bundle"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@wasm-tool/rollup-plugin-rust": "^1.0.7",
    "cypress": "^13.2.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-svelte3": "^3.4.1",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "rollup": "^2.79.1",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-svelte": "^7.1.6",
    "sirv-cli": "^1.0.14",
    "svelte": "^3.59.2",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-terser": "^0.4.3"
  },
  "type": "module",
  "dependencies": {}
}
